{
  "name": "matebot_js",
  "version": "1.0.0",
  "description": "- Bienvenida a los usuarios (newMembers.py) - Reminder(scheduler) - FAQ's (faq.py)",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clearDistDirectory",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon",
    "clearDistDirectory": "rimraf dist",
    "migrate": "npm run typeorm migration:run",
    "syncDb": "npm run typeorm schema:sync",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hifrontendcafe/matebotJS.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hifrontendcafe/matebotJS/issues"
  },
  "homepage": "https://github.com/hifrontendcafe/matebotJS#readme",
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typedi": "^0.10.0",
    "winston": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  }
}
